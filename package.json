{
  "name": "docs",
  "version": "0.0.7",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && tsc --noEmit && astro build",
    "test": "vitest",
    "preview": "astro preview",
    "check": "astro check",
    "tsc": "astro sync && tsc",
    "format": "pnpm run format:code",
    "format:ci": "pnpm run format:imports && pnpm run format:code",
    "format:code": "prettier -w . --cache --plugin-search-dir=.",
    "format:imports": "organize-imports-cli ./tsconfig.json",
    "add-language": "node ./scripts/add-language.mjs",
    "docgen": "node ./scripts/docgen.mjs",
    "docgen:errors": "node ./scripts/error-docgen.mjs",
    "lint:a11y": "start-test 'yarn build && yarn preview' 3000 'yarn lint:a11y:local'",
    "lint:a11y:local": "pa11y-ci --sitemap 'http://localhost:3000/sitemap.xml' --sitemap-find 'https://docs.oxyprops.com' --sitemap-replace 'http://localhost:3000'",
    "lint:a11y:remote": "pa11y-ci --sitemap 'https://docs.oxyprops.com/sitemap.xml'",
    "lint:linkcheck": "SKIP_OG=true pnpm build && tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:linkcheck:nobuild": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/lint-linkcheck.ts",
    "lint:slugcheck": "node ./scripts/lint-slugcheck.mjs",
    "lint:eslint": "eslint .",
    "netlify:build": "pnpm ${NETLIFY_BUILD_SCRIPT:-build}",
    "translation-status": "tsm --require=./scripts/lib/filter-warnings.cjs ./scripts/translation-status.ts"
  },
  "devDependencies": {
    "@11ty/eleventy-fetch": "^4.0.0",
    "@actions/core": "^1.10.0",
    "@algolia/client-search": "^4.17.0",
    "@astrojs/mdx": "^0.19.1",
    "@astrojs/preact": "^2.1.0",
    "@astrojs/sitemap": "1.3.0",
    "@babel/core": "^7.21.8",
    "@docsearch/css": "^3.3.4",
    "@docsearch/react": "^3.3.4",
    "@types/canvas-confetti": "^1.6.0",
    "@types/chroma-js": "^2.4.0",
    "@types/hast": "^2.3.4",
    "@types/html-escaper": "^3.0.0",
    "@types/mdast": "^3.0.11",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "astro": "^2.4.2",
    "astro-auto-import": "^0.3.0",
    "astro-eslint-parser": "^0.13.3",
    "astro-og-canvas": "^0.1.8",
    "bcp-47-normalize": "^2.2.0",
    "canvaskit-wasm": "^0.38.1",
    "chroma-js": "^2.4.2",
    "dedent-js": "^1.0.1",
    "domhandler": "^5.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-astro": "^0.26.1",
    "eslint-plugin-react": "^7.32.2",
    "fast-glob": "^3.2.12",
    "hast-util-from-html": "^1.0.2",
    "hast-util-to-string": "^2.0.0",
    "hastscript": "^7.2.0",
    "html-escaper": "^3.0.3",
    "htmlparser2": "^8.0.2",
    "kleur": "^4.1.5",
    "mdast-util-from-markdown": "^1.3.0",
    "mdast-util-mdx-jsx": "^2.1.2",
    "mdast-util-to-hast": "^12.3.0",
    "mdast-util-to-string": "^3.2.0",
    "micromark-util-character": "^1.1.0",
    "micromark-util-symbol": "^1.0.1",
    "node-fetch": "^3.3.1",
    "organize-imports-cli": "^0.10.0",
    "p-retry": "^5.1.2",
    "parse-numeric-range": "^1.3.0",
    "preact": "^10.13.2",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
    "prompts": "^2.4.2",
    "rehype": "^12.0.1",
    "remark": "^14.0.2",
    "remark-directive": "^2.0.1",
    "simple-git": "^3.18.0",
    "tsm": "^2.3.0",
    "typescript": "^5.0.4",
    "unified": "^10.1.2",
    "unist-util-remove": "^3.1.1",
    "unist-util-visit": "^4.1.2",
    "vfile": "^5.3.7",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "@astrojs/image": "^0.16.7",
    "@fontsource/ibm-plex-mono": "^4.5.13",
    "@github/clipboard-copy-element": "^1.1.2",
    "@nanostores/preact": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/pluginutils": "^5.0.2",
    "astro-icon": "^0.8.0",
    "canvas-confetti": "^1.6.0",
    "jsdoc-api": "^8.0.0",
    "nanostores": "^0.8.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "remark-smartypants": "^2.0.0",
    "sass": "^1.62.1",
    "sharp": "^0.32.1"
  }
}
